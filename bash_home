# Add dart executables to PATH
export PATH=$PATH:/usr/lib/dart/bin
export PATH=$PATH:$HOME/.pub-cache/bin

# Add dart protobuf plugin to PATH
export PATH=$PATH:$HOME/opt/dart-protoc-plugin/out

# Add cabal packages to PATH
export PATH=$PATH:$HOME/.cabal/bin

# Add android to PATH
export PATH=$PATH:$HOME/opt/android-sdk-linux/tools
export PATH=$PATH:$HOME/opt/android-sdk-linux/platform-tools
